{
  "name": "driver-matching-service",
  "version": "1.0.0",
  "description": "Driver matching service for AWS Lambda",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --external:@aws-sdk/* --sourcemap",
    "build:local": "esbuild src/index.local.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --external:@aws-sdk/* --sourcemap",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "lambda",
    "aws",
    "driver-matching"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-lambda-powertools/tracer": "^2.28.1",
    "@aws-sdk/client-dynamodb": "^3.709.0",
    "@aws-sdk/client-eventbridge": "^3.709.0",
    "@aws-sdk/lib-dynamodb": "^3.709.0"
  }
}
